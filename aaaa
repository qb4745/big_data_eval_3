  --runtime python311 \
  --trigger-resource registros-produccion \
  --trigger-event google.pubsub.topic.publish \
  --source=./procesamiento \
  --entry-point=main \
  --region=us-central1 \
  --set-env-vars="GCP_PROJECT=$(gcloud config get-value project)" \
  --retry \
  --no-gen2


